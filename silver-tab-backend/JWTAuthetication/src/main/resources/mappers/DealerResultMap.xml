<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.BYD.mapper.DealerMapper">
    <!-- Result map for dealer information -->
    <resultMap id="dealerInfoResultMap" type="com.BYD.dto.dealer.DealerInfoDTO">
        <!-- Main info -->
        <result column="DEALER_NUMBER" property="dealerNumber"/>
        <result column="DEALER_CODE" property="dealerCode"/>
        <result column="DEALER_NAME" property="dealerName"/>
        <result column="CORPORATE_NAME" property="corporateName"/>
        <result column="GROUP_NAME" property="groupName"/>
        <result column="REGION" property="region"/>
        <result column="REGIONAL_MANAGER_NAME" property="regionalManagerName"/>
        <result column="STATE" property="state"/>
        <result column="CITY" property="city"/>
        <result column="ADDRESS" property="address"/>
        <result column="COUNTRY" property="country"/>
        <result column="PASSED_TRAINING" property="passedTraining"/>
        <result column="LONGITUDE" property="longitude"/>
        <result column="LATITUDE" property="latitude"/>
        <result column="CONTACT_NUMBER" property="contactNumber"/>
        <result column="EMAIL" property="email"/>

        <!-- Operation info -->
        <result column="OPERATION_TYPE_NAME" property="operationTypeName"/>
        <result column="OPERATION_STATUS_NAME" property="operationStatusName"/>
        <result column="OPERATION_CLASS_NAME" property="operationClassName"/>
        <result column="OPERATION_SERVICE_SCOPE_NAME" property="operationServiceScopeName"/>
        <result column="OPERATION_DATE" property="operationDate"/>
        <result column="OPERATION_AFTER_SALE" property="operationAfterSale"/>
        <result column="OPERATION_AFTER_SALE_NOTE" property="operationAfterSaleNote"/>

        <!-- System usage -->
        <result column="DEALER_USE_DMS" property="dealerUseDms"/>
        <result column="DEALER_USE_LMS" property="dealerUseLms"/>
        <result column="DEALER_USE_TIS" property="dealerUseTis"/>
        <result column="DEALER_USE_CSI_PORTAL" property="dealerUseCsiPortal"/>
        <result column="DEALER_USE_BYD_PORTAL" property="dealerUseBydPortal"/>

        <!-- Key position quantities -->
        <result column="AFTER_SALES_MANAGER_QUANTITY" property="afterSalesManagerQuantity"/>
        <result column="SERVICE_ADVISOR_QUANTITY" property="serviceAdvisorQuantity"/>
        <result column="WARRANTY_CLAIM_SPECIALIST_QUANTITY" property="warrantyClaimSpecialistQuantity"/>
        <result column="TECHNICIANS_QUANTITY" property="techniciansQuantity"/>
        <result column="WORKSHOP_LEADER_QUANTITY" property="workshopLeaderQuantity"/>
        <result column="SPARE_PARTS_SPECIALIST_QUANTITY" property="sparePartsSpecialistQuantity"/>

        <!-- Facilities and tools -->
        <result column="TOTAL_AREA" property="totalArea"/>
        <result column="WORKSHOP_AREA" property="workshopArea"/>
        <result column="TOTAL_BOX" property="totalBox"/>
        <result column="BOX_WITH_LIFT" property="boxWithLift"/>
        <result column="BOX_WITHOUT_LIFT" property="boxWithoutLift"/>
        <result column="AC_CHARGER_QUANTITY" property="acChargerQuantity"/>
        <result column="DC_CHARGER_QUANTITY" property="dcChargerQuantity"/>
        <result column="STATUS_VDS" property="statusVds"/>
        <result column="BATTERY_POOL" property="batteryPool"/>
        <result column="BODY_REPAIR_WORKSHOP_AREA" property="bodyRepairWorkshopArea"/>
        <result column="BODY_REPAIR_STATION_AREA" property="bodyRepairStationArea"/>
        <result column="WAREHOUSE_AREA" property="warehouseArea"/>
        <result column="POWER_BATTERY_TURNOVER_ROOM_AREA" property="powerBatteryTurnoverRoomArea"/>
        <result column="RECEPTION_AREA" property="receptionArea"/>
        <result column="CUSTOMER_LOUNGE_AREA" property="customerLoungeArea"/>
        <result column="CUSTOMER_TOILET_AREA" property="customerToiletArea"/>
        <result column="CHARGING_AREA" property="chargingArea"/>
        <result column="POWERTRAIN_REPAIR_ROOM_AREA" property="powertrainRepairRoomArea"/>
        <result column="MOBILE_FIRE_WATER_POOL_AREA" property="mobileFireWaterPoolArea"/>
        <result column="SERVICE_DIRECTOR_OFFICE_AREA" property="serviceDirectorOfficeArea"/>
        <result column="BUSINESS_DEVELOPMENT_CENTER_AREA" property="businessDevelopmentCenterArea"/>
        <result column="MEETING_TRAINING_ROOM_AREA" property="meetingTrainingRoomArea"/>
        <result column="WORKSHOP_OFFICE_AREA" property="workshopOfficeArea"/>
        <result column="STAFF_LOUNGE_AREA" property="staffLoungeArea"/>
        <result column="TOOL_ROOM_AREA" property="toolRoomArea"/>
        <result column="WASTE_OIL_AREA" property="wasteOilArea"/>
        <result column="ELECTRICITY_DISTRIBUTION_ROOM_AREA" property="electricityDistributionRoomArea"/>
        <result column="AIR_COMPRESSOR_ROOM_AREA" property="airCompressorRoomArea"/>
        <result column="WORKSHOP_TOILET_AREA" property="workshopToiletArea"/>
        <result column="PAINT_STORAGE_BLENDING_ROOM_AREA" property="paintStorageBlendingRoomArea"/>
        <result column="CAR_WASHING_AREA" property="carWashingArea"/>
        <result column="CLAIM_PARTS_STORAGE_ROOM_AREA" property="claimPartsStorageRoomArea"/>
        <result column="FINANCE_OFFICE_AREA" property="financeOfficeArea"/>
        <result column="INSURANCE_CLAIM_AREA" property="insuranceClaimArea"/>
        <result column="DISASSEMBLED_PARTS_ROOM_AREA" property="disassembledPartsRoomArea"/>
        <result column="USED_PARTS_STORAGE_AREA" property="usedPartsStorageArea"/>
    </resultMap>

    <resultMap id="conciseDealerInfoResultMap" type="com.BYD.dto.dealer.ConciseDealerInfoDTO">
        <!-- Main info -->
        <result column="DEALER_CODE" property="dealerCode"/>
        <result column="DEALER_NAME" property="dealerName"/>
        <result column="GROUP_NAME" property="groupName"/>
        <result column="REGION" property="region"/>
        <result column="REGIONAL_MANAGER_NAME" property="regionalManagerName"/>
        <result column="CONTACT_NUMBER" property="contactNumber"/>
        <result column="EMAIL" property="email"/>
        <result column="OPERATION_STATUS_NAME" property="operationStatusName"/>
        <result column="OPERATION_SERVICE_SCOPE_NAME" property="operationServiceScopeName"/>
        <result column="OPERATION_AFTER_SALE" property="operationAfterSale"/>
    </resultMap>
</mapper>